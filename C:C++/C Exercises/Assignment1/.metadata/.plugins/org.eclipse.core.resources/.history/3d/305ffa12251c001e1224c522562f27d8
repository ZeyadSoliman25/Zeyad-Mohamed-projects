#include <stdio.h>
int main(){
	int num1 = 0;
	int num2 = 0;
	float result= 0;
	char op;
	printf("Enter two numbers: ");
	scanf("%d %d", &num1, &num2);
	printf("Enter operation (+, - , *, /): ");
	scanf("%c", op);
	switch (op){
	case "+":
		result = num1 + num2 ;
		printf("The result = %f", result);
		break;
	case "-":
		result = num1 - num2;
		printf("The result = %f", result);
		break;
	case "*":
		result = num1 * num2;
		printf("The result = %f", result);
		break;
	case "/":
		result = num1 / num2;
		printf("The result = %f", result);
		break;
	}

}
