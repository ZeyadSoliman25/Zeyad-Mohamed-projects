#include <stdio.h>
int main(){
	char name = 0;
	char grade = 0 ;
	printf("Enter your name and grade: \n");
	name = getchar ();
	getchar();
	grade = getchar ();
	getchar();
	printf("Hello %s \n",name);
	printf("Nice %c grade champ\n",grade);

}
