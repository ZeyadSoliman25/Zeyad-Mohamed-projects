#include <stdio.h>
int main()
{
	int i = 0 ,j,h;
	int l = 0 ,m,n;
	for (l=1 ; l<=5; l++){
		for (m=1 ; m<=l; m++){
			printf(" ");
		}
		if (l>=1){
			printf("*");
		}
		for (n=11 ; n>=2*l-2; n--){
			printf(" ");
		}
		if (l>=1){
			printf("*");
		}
		printf("\n");

	}
	for (i = 1; i<=5; i++){
		for (h=5 ; h>=i ; h--){
			printf(" ");
		}
		if (i>1){
			printf("*");
		}
		for (j=1 ; j<= 2*i - 1 ; j++){
			printf(" ");
		}
		if (i>1){
			printf("*");
		}
		printf("\n");
	}
}
