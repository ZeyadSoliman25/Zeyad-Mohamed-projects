#include <stdio.h>
#include <math.h>
int main(){
	int num = 0;
	printf("Enter a number: ");
	scanf("%d", &num);
	if ((sqrt(num) % 1) > 0){
		printf("The number is not a perfect square.");
	}
	else {
		printf("The number is a perfect square.");
	}
}
