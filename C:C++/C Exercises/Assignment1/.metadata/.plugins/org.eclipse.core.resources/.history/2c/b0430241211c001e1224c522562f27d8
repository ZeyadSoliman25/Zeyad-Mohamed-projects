#include <stdio.h>
#include <math.h>
int main(){
	double num = 0;
	double sqrt_num = 0;
	printf("Enter a number: ");
	scanf("%lf", &num);
	sqrt_num = num/sqrt(num);
	if (sqrt_num == sqrt(num)){
		printf("The number is a perfect square.");
	}
	else {
		printf("The number is not a perfect square.");
	}
}
