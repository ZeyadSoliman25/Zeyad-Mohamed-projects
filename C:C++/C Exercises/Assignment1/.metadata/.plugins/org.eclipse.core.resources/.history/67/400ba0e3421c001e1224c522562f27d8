#include <stdio.h>
int main()
{
	int i = 0 ,j,h, l, m, n;
	for (l=5 ; l<=1; l--);{
		if (l>1){
			printf("*");
		}
		for (n=1 ; j<= 2*l - 1 ; n++){
			printf(" ");
		}
		if (l>1){
			printf("*");
		}
		printf("\n");

	}
	for (i = 1; i<=5; i++){
		if (i>1){
			printf("*");
		}
		for (j=1 ; j<= 2*i - 1 ; j++){
			printf(" ");
		}
		if (i>1){
			printf("*");
		}
		printf("\n");
	}
}
