#include <stdio.h>


void fibonacci_series (int num);


int main(){
	int num = 0;
	fibonacci_series(num);
}

void fibonacci_series (int num){
	int sum = 0;
	do {
		printf("%d,", num);
		num +=1;
		sum = num;
	}while (num<2);
	if (1 == sum){
		sum = 0;
		num = 1;
	}

}
