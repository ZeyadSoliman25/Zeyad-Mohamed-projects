#include <stdio.h>


void fibonacci_series (int num);


int main(){
	int num = 0;
	fibonacci_series(num);
}

void fibonacci_series (int num){
	int sum = 0;
	if (num<1){
		for (int i = 0; i <=1; i++){
			printf("%d, ",i);
		}
		num = 1;
	}
	sum = num;
	printf("%d, ",num);
	num = sum;
	fibonacci_series(num);

}
