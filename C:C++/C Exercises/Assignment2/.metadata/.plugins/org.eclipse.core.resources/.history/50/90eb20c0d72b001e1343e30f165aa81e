#include <stdio.h>

int prime(int num1,int num2);

int main(){
	int num1 = 0;
	int num2 = 0;
	printf("Enter two numbers: ");
	scanf("%d %d", &num1, &num2);
	printf("The prime numbers between intervals: \n");
	for (num1; num1<=num2;num1++){
		printf("%d", prime(num1,num2));
	}
}

int prime(num1, num2){
	for (int j=num1; j<= num2; j++){
		for (int i=0; i<=j; i++){
			if (j%i == 0 && j==i){
				return i;
			}
		}
	}
	return 0;
}
