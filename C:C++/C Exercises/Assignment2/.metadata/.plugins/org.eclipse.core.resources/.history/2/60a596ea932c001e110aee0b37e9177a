#include <stdio.h>


void fibonacci_series (int num);


int main(){
	int num = 0;
	fibonacci_series(num);
}

void fibonacci_series (int num){
	int sum;
	if (num<1){
		for (int i = 0; i <=1; i++){
			printf("%d, ",i);
			num ++;
		}
		sum = 0;
	}
	printf("%d, ",num);
	sum = num;
	num +=sum;
	fibonacci_series(num);

}
