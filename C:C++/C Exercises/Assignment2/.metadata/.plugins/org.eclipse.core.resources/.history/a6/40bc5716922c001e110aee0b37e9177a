#include <stdio.h>


void fibonacci_series (int num);


int main(){
	int num = 0;
	fibonacci_series(num);
}

void fibonacci_series (int num){
	int sum = 0;
	while (sum<2){
		printf("%d, ", num);
		sum += 1;
		num +=1;
		if (sum == 2){
			sum = 0;
			num = 1;
			break;
		}
	}
	sum += num;
	num = sum;
	printf("%d, ", sum);

}
