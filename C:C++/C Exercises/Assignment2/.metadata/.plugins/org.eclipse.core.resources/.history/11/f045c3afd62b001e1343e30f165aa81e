#include <stdio.h>

int prime(int num1,int num2);

int main(){
	int num1 = 0;
	int num2 = 0;
	int prime_numbers[10];
	printf("Enter two numbers: ");
	scanf("%d %d", &num1, &num2);
	printf("The prime numbers between intervals: \n");
	prime_numbers = prime(num1, num2);
	printf("%d", prime_numbers);
}

int prime(num1, num2){
	int prime_numbers[10];
	int counter = 0;
	for (int j=num1; j<= num2; j++){
		for (int i=0; i<=j; i++){
			if (j%i == 0 && j==i){
				prime_numbers[counter] = i;
				counter++;
			}
		}
	}
	return counter;
}
