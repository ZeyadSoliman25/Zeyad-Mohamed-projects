#include <stdio.h>


void fibonacci_series (int num);


int main(){
	int num = 0;
	fibonacci_series(num);
}

void fibonacci_series (int num){
	int sum = 0;
	if (0 == num){
		printf("%d,", num);
		fibonacci_series(num+1);
	}
	else {
		num += sum;
		sum = num;
		printf("%d,", num);
		fibonacci_series(num);
	}
}
