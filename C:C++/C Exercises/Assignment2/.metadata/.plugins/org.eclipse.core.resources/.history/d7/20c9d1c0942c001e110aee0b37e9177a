#include <stdio.h>


void fibonacci_series (int num);


int main(){
	int num = 0;
	fibonacci_series(num);
}

void fibonacci_series (int num){
	int sum = num;
	if (num<1){
		for (int i = 0; i <=1; i++){
			printf("%d, ",i);
		}
		sum = 1;
	}
	printf("%d, ",sum);
	sum += num;
	fibonacci_series(sum);

}
