#include <stdio.h>


void fibonacci_series (int num);


int main(){
	int num = 0;
	fibonacci_series(num);
}

void fibonacci_series (int num){
	int sum = sum;
	int num2 = num;
	if (num2<1){
		for (int i = 0; i <=1; i++){
			printf("%d, ",i);
		}
		num = 1;
		num2 = 1;
		printf("%d", num);
	}
	sum = num + num2;
	printf("%d, ",sum);
	fibonacci_series(sum);

}
