#include <stdio.h>

void reverse_number(unsigned char *num1, unsigned char *num2){
	*num2[0]=*num1[7];
	*num2[1]=*num1[6];
	*num2[2]=*num1[5];
	*num2[3]=*num1[4];
	*num2[4]=*num1[3];
	*num2[5]=*num1[2];
	*num2[6]=*num1[1];
	*num2[7]=*num1[0];
}

int main(void){
	unsigned char num1 = 0x11;
	unsigned char num2 = 0;
	reverse_number(&num1, &num2);
	printf("%d\n %d", num1, num2);
}


